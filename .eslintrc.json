{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-return-await": 0,
    "prettier/prettier": ["error", { "singleQuote": true }],
    "prefer-destructuring": 0,
    "import/extensions": 0,
    "no-underscore-dangle": 0
  }
}
